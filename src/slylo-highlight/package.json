{
  "name": "slylo-highlight",
  "displayName": "Slylo Highlight",
  "description": "Highlight support for Stylo language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "stylo",
      "aliases": ["Stylo", "stylo"],
      "extensions": [".stylo"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "stylo",
      "scopeName": "source.stylo",
      "path": "./syntaxes/stylo.tmLanguage.json"
    }]
  }
}
