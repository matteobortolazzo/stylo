{
  "name": "lsp",
  "displayName": "Stylo",
  "description": "Stylo support",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "stylo",
        "aliases": [
          "Stylo",
          "stylo"
        ],
        "extensions": [
          "stylo"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "stylo",
        "scopeName": "source.stylo",
        "path": "./syntaxes/stylo.tmLanguage.json"
      }
    ]
  },
  "dependencies": {
    "vscode-languageclient": "^8.1.0",
    "vscode-languageserver": "^8.1.0"
  }
}
